{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Remote Debug via SSH",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/quakedb",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "pipeTransport": {
                "pipeProgram": "ssh -i /c/dev/.ssh/id_rsa     -o UserKnownHostsFile=/c/dev/.ssh/known_hosts     -o GlobalKnownHostsFile=/dev/null     -o StrictHostKeyChecking=no",
                "pipeArgs": [
                    "osboxes@127.0.0.1",
                    "-T"
                ],
                "debuggerPath": "/usr/bin/gdb",
                "pipeCwd": "${workspaceFolder}"
            },
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "sourceFileMap": {
                "/home/osboxes": "${workspaceFolder}"
            }
        }
    ]
}
